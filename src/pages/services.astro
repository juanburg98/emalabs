---
import Layout from "../layouts/Layout.astro";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
---

<Layout title="Servicios | Emalabs">
  <header>
    <nav>
      <Navbar />
    </nav>
  </header>
  <main>
    <div class="flex justify-center">
      <h2
        class="m-1 w-1/2 pt-28 text-center font-serif text-5xl font-bold tracking-title text-primary"
      >
        Estos son nuestros servicios
      </h2>
    </div>
  </main>
  <footer>
    <Footer />
  </footer>
</Layout>
